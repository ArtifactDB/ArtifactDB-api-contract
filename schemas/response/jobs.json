{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "jobs.json",
    "type": "object",
    "title": "Job response",
    "description": "JSON body of the response for the `GET /jobs/{jobid}` endpoint.",
    "properties": {
        "status": {
            "type": "string",
            "description": "State of the post-upload job.",
            "enum": [ "SUCCESS", "FAILURE", "PENDING" ]
        }
    },
    "required": [
        "status"
    ]
}
